#include<stdio.h>
#include<math.h>
int main()
{
    int num1,num2;
    printf("\n ENTER THE NUMBER1:");
    scanf("%d",&num1);
    printf("\n ENTER THE NUMBER2:");
    scanf("%d",&num2);
    if(num1>num2)
    {
        printf("\n THE MAXIMUM NUMBER1 IS %d",num1);
        printf("\n THE MINIMUM NUMBER2 IS %d",num2);
    }
    else (num2>num1);
    {
        printf("\n THE MAXIMUM NUMBER2 IS %d",num2);
        printf("\n THE MINIMUM NUMBER1 IS %d",num1);
    }
    return 0;
}
